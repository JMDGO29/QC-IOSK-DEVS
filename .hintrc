{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "dialog",
          "meta[name=color-scheme]",
          "input[type=time]",
          "input[type=date]"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "manifest-file-extension": "off",
    "meta-viewport": "off",
    "apple-touch-icons": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "typescript-config/strict": "off",
    "typescript-config/consistent-casing": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "disown-opener": "off",
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-command-name": "off",
        "aria-progressbar-name": "off",
        "aria-required-parent": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off",
        "duplicate-id": "off"
      }
    ]
  }
}